img>\r\n            <p>© 2020 Kasa. All rights reserved</p>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Footer;","C:\\react projets\\P11\\kasa\\src\\components\\Header.js",["82"],[],"import React from 'react';\r\nimport Logo from '../assets/Logo.JPG';\r\nimport Nav from './Nav';\r\n\r\nfunction Header() {\r\n    return (\r\n        <div className='div-app-header'>\r\n            <img className='logo-header' src={Logo}></img>\r\n            <Nav />\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Header;","C:\\react projets\\P11\\kasa\\src\\components\\Tag.js",[],[],"C:\\react projets\\P11\\kasa\\src\\components\\Lightbox.js",["83"],[],"import React, { useState } from 'react';\r\n\r\nfunction Lightbox(props) {\r\n\r\n    const [dataImage, setDataImage] = useState(props.data[0])\r\n    const [indexImage, setindexImage] = useState(0)\r\n\r\n    function nextImage() {\r\n        if (indexImage === props.data.length -1) {\r\n            setindexImage(0);\r\n            setDataImage(props.data[0]);\r\n        } else {\r\n            setDataImage(props.data[indexImage+1]);\r\n            setindexImage(indexImage+1);\r\n        }\r\n    }\r\n    function previousImage() {\r\n        if (indexImage === 0) {\r\n            setDataImage(props.data[props.data.length -1]);\r\n            setindexImage(props.data.length -1);\r\n        } else {\r\n            setDataImage(props.data[indexImage-1]);\r\n            setindexImage(indexImage-1);\r\n        }\r\n    }\r\n    \r\n    return (\r\n        <div className='div-lightbox'>\r\n            <img src={dataImage}></img>\r\n            {props.data.length > 1 ? \r\n            <div>\r\n                <i onClick={previousImage} className=\"fa fa-solid fa-angle-left\"></i> \r\n                <i onClick={nextImage} className =\"fa fa-solid fa-angle-right\"></i>\r\n            </div> : ''}\r\n            <p>{indexImage+1}/{props.data.length}</p>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Lightbox;","C:\\r